    "react-dom": "^16.0.0",
    "regenerator-runtime": "^0.14.0"
  },
  "peerDependencies": {
    "react": ">=16.9.0",
    "react-dom": ">=16.9.0"
  },
  "resolutions": {
		"@types/minimatch": "5.1.2"
	},
  "cnpm": {
    "mode": "npm"
